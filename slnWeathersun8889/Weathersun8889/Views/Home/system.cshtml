@model IEnumerable<Weathersun8889.系統回報資料>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -晴穿搭</title>
    <link rel="sun-touch-icon" sizes="76x76" href="~/img/sun-icon.png">
    <link rel="icon" type="image/png" href="~/img/weathersun.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>
        晴穿搭
    </title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <!-- CSS Files -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/paper-dashboard.css?v=2.0.1" rel="stylesheet" />
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/Content/demo/demo.css" rel="stylesheet" />
</head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.css">
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.js"></script>
<script>
    $(function () {
        $('#myTable').DataTable();
    });
</script>
@{
    ViewBag.Title = "系統回報資料";
}

<body class="">
    <div class="wrapper ">
        <div class="sidebar" data-color="white" data-active-color="danger">
            <div class="logo">
                <a class="simple-text logo-mini">
                    <div class="logo-image-small">
                        <img src="~/img/logo-sun.png">
                    </div>
                </a>
                <a class="simple-text logo-normal">
                    <strong>
                        晴穿搭                  
                    </strong>
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="Index1">

                            <h6>👦🏻 會員管理平台</h6>
                        </a>
                    </li>

                    <li>
                        <a href="manager">

                            <h6>👩‍🔧 管理員平台</h6>
                        </a>
                    </li>
                    <li>
                        <a href="announcement">

                            <h6>📜 公告事項</h6>
                        </a>
                    </li>
                    <li class="active ">
                        <a href="system">

                            <h5><strong>📰 系統回報資料</strong></h5>
                        </a>
                    </li>
                    <li>
                        <a href="Advise">

                            <h6>✉ 網頁意見回饋資料表</h6>
                        </a>
                    </li>
                    <li>
                        <a href="identity">

                            <h6>👥 身分驗證資料</h6>
                        </a>
                    </li>
                    <li>
                        <a href="products">

                            <h6>👕 商品</h6>
                        </a>
                    </li>
                    <li>
                        <a href="proreclist">

                            <h6>🛍 商品推薦表</h6>
                        </a>
                    </li>
                    <li>
                        <a href="peoplehead">

                            <h6>🧍‍ 虛擬人像</h6>
                        </a>
                    </li>
                    <li>
                        <a href="douctment">

                            <h6>📱 貼文管理</h6>
                        </a>
                    </li>
                    <li>
                        <a href="clothes">

                            <h6>👗 穿搭資料</h6>
                        </a>
                    </li>
                    <li class="active-pro">

                        <p>&nbsp;&nbsp;&copy;@DateTime.Now.Year - 晴穿搭</p>
                    </li>
                </ul>
            </div>
        </div>
    <div class="main-panel">
        <!-- Navbar -->
        <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
            <div class="container-fluid">
                <div class="navbar-wrapper">
                    <div class="navbar-toggle">
                        <button type="button" class="navbar-toggler">
                            <span class="navbar-toggler-bar bar1"></span>
                            <span class="navbar-toggler-bar bar2"></span>
                            <span class="navbar-toggler-bar bar3"></span>
                        </button>
                    </div>
                    <a class="navbar-brand" href="javascript:;"><strong>晴穿搭後臺管理⛅</strong></a>
                </div>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-bar navbar-kebab"></span>
                    <span class="navbar-toggler-bar navbar-kebab"></span>
                    <span class="navbar-toggler-bar navbar-kebab"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-end" id="navigation">

                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link btn-rotate" href="#">

                                <p>
                                    使用守則
                                </p>
                            </a>
                        <li class="nav-item btn-rotate dropdown">
                            <a class="nav-link dropdown-toggle" href="http://example.com" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">

                                <p>
                                    登入
                                </p>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                                <a class="dropdown-item" href="Index3">管理員</a>
                                <a class="dropdown-item" href="Index">會員</a>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link btn-rotate" href="Index3">

                                <p>
                                    登出
                                </p>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <table id="myTable" class="display">
                                <thead>
                                    <tr style="background-color:#f0f8ff;">
                                        @Html.ActionLink("新增系統回報資料 ", "systemCreate", new { @class = "btn btn-default" })
                                        <th>
                                            @Html.DisplayNameFor(model => model.會員帳號)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.回報內容)
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.會員帳號)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.回報內容)
                                            </td>
                                            <td>
                                                @Html.ActionLink("📃", "Edit", new { account = item.會員帳號 })

                                                @Html.ActionLink("❌", "Delete", new { account = item.會員帳號 },
                                                new { onclick = "return confirm('確定刪除嗎?');" })
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <h5>&nbsp;</h5>
        <h2>&nbsp;</h2>
        <div class="credits ml-auto">
            <div class="container body-content">

                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year - 晴穿搭</p>
                </footer>
            </div>
        </div>
    </div>
  </div>
    <!--   Core JS Files   -->
    <script src="~/Scripts//js/core/jquery.min.js"></script>
    <script src="~/Scripts//js/core/popper.min.js"></script>
    <script src="~/Scripts//js/core/bootstrap.min.js"></script>
    <script src="~/Scripts//js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <!--  Google Maps Plugin    -->
    <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
    <!-- Chart JS -->
    <script src="~/Scripts//js/plugins/chartjs.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="~/Scripts//js/plugins/bootstrap-notify.js"></script>
    <!-- Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="~/Scripts//js/paper-dashboard.min.js?v=2.0.1" type="text/javascript"></script><!-- Paper Dashboard DEMO methods, don't include it in your project! -->
    <script src="~/Scripts//demo/demo.js"></script>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.css">
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.js"></script>
    <script>
        $(document).ready(function () {
            // Javascript method's body can be found in assets/assets-for-demo/js/demo.js
            demo.initChartsPages();
        });
    </script>
</body>