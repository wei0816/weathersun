@model IEnumerable<Weathersun8889.貼文>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
    <form style="margin-left:10px;">
        <div class="form-group">
            <label for="exampleInputEmail1">貼文</label>
            <input type="text" class="form-control" id="poststring"  placeholder="Enter string" value="貼文文字">
            <small id="postsyring" class="form-text text-muted">說點什麼吧~</small>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">照片</label>
            <input type="text" class="form-control" id="postimg" placeholder="photo" value="貼文照片">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">標籤</label>
            <input type="text" class="form-control" id="posttag" aria-describedby="emailHelp" placeholder="hasgtag" value="商品標籤">
        </div>
        <button type="submit" class="btn btn-primary">確定</button>
    </form>